config_version=5

[application]

config/name="NIO's Job"
run/main_scene="res://scenes/level/make_burger/burger_main_desktop.tscn"
config/features=PackedStringArray("4.3", "Mobile")
run/max_fps=60
config/icon="res://assets/images/sprites/Nio/normal/idle_move/Nio_f_walk.png"
run/snap/snap_2d_transforms_to_pixel=true

[autoload]

GlobalSignalBus="*res://scripts/global_scripts/GlobalSignalBus.gd"
GlobalVarBus="*res://scripts/global_scripts/GlobalVarBus.gd"
SceneManager="*res://scripts/system/SceneManager.gd"

[display]

window/size/viewport_width=2560
window/size/viewport_height=1440
window/size/window_width_override=1920
window/size/window_height_override=1080
window/stretch/mode="viewport"

[editor]

export/convert_text_resources_to_binary=false

[editor_plugins]

enabled=PackedStringArray("res://addons/gdscript_formatter/plugin.cfg")

